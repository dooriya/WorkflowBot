{
    "type": "AdaptiveCard",
    "body": [
      {
        "type": "TextBlock",
        "size": "Medium",
        "weight": "Bolder",
        "text": "${title}"
      },
      {
        "type": "TextBlock",
        "text": "${body}",
        "wrap": true
      },
      {
        "type": "ActionSet",
        "actions": [
          {
            "type": "Action.OpenUrl",
            "title": "Bot Framework Docs",
            "url": "https://docs.microsoft.com/en-us/azure/bot-service/?view=azure-bot-service-4.0"
          },
          {
            "type": "Action.OpenUrl",
            "title": "Teams Toolkit Docs",
            "url": "https://aka.ms/teamsfx-docs"
          },
          {
            "type": "Action.Execute",
            "verb": "doAction",
            "title": "doAction",
            "data": {
                "key1": "value1"
            }
          }
        ]
      }
    ],
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.4"
  }
  